  postgres-container:
    environment:
      POSTGRES_DB: eCommerceUsers
      POSTGRES_PASSWORD: admin
      POSTGRES_USER: postgres
    image: postgres:13
    networks:
      users-postgres-network: null
    ports:
      - mode: ingress
        target: 5432
        published: "5433"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\aacis\source\repos\microservicesWithAzureDevOps\eCommerce\postgres-init
        target: /docker-entrypoint-initdb.d
        bind:
          create_host_path: true
  products-microservice:
