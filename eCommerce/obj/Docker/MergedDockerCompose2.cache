      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      ecommerce-network: null
    ports:
      - mode: ingress
        target: 8080
        published: "4000"
        protocol: tcp
    tty: true
    volumes:
      - type: bind
        source: C:\Users\aacis\vsdbg\vs2017u5
        target: /remote_debugger
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\aacis\AppData\Roaming\ASP.NET\Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\aacis\AppData\Roaming\ASP.NET\Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\aacis\AppData\Roaming\Microsoft\UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\aacis\AppData\Roaming\Microsoft\UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Program Files\Microsoft Visual Studio\2022\Community\MSBuild\Sdks\Microsoft.Docker.Sdk\tools\linux-x64\net8.0
        target: /VSTools
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Program Files\Microsoft Visual Studio\2022\Community\Common7\IDE\CommonExtensions\Microsoft\HotReload
        target: /HotReloadAgent
        read_only: true
        bind:
          create_host_path: true
  mongodb-container:
    image: mongo:latest
    networks:
      orders-mongodb-network: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\aacis\source\repos\microservicesWithAzureDevOps\eCommerce\mongodb-init
        target: /docker-entrypoint-initdb.d
        bind:
          create_host_path: true
  mysql-container:
    environment:
      MYSQL_ROOT_PASSWORD: admin
    image: mysql:8.0
    networks:
      products-mysql-network: null
    ports:
      - mode: ingress
        target: 3306
        published: "3307"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\Users\aacis\source\repos\microservicesWithAzureDevOps\eCommerce\mysql-init
        target: /docker-entrypoint-initdb.d
        bind:
          create_host_path: true
  ordersmicroservice-api:
