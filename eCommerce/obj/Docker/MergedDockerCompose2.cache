    ports:
      - mode: ingress
        target: 8080
        published: "6000"
        protocol: tcp
  rabbitmq:
    environment:
      RABBITMQ_DEFAULT_PASS: password
      RABBITMQ_DEFAULT_USER: user
    image: rabbitmq:3.13-management
    networks:
      ecommerce-network: null
    ports:
      - mode: ingress
        target: 5672
        published: "5672"
        protocol: tcp
      - mode: ingress
        target: 15672
        published: "15672"
        protocol: tcp
  redis:
    image: redis:latest
    networks:
      ecommerce-network: null
    ports:
      - mode: ingress
        target: 6379
        published: "6379"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\aacis\source\repos\microservicesWithAzureDevOps\eCommerce\redis-cache
        target: /data
        bind:
          create_host_path: true
  redisinsight:
    depends_on:
      redis:
        condition: service_started
        required: true
    image: redis/redisinsight:latest
    networks:
      ecommerce-network: null
    ports:
      - mode: ingress
        target: 5540
        published: "5540"
        protocol: tcp
  users-microservice:
    depends_on:
      postgres-container:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_HTTP_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      POSTGRES_DATABASE: eCommerceUsers
      POSTGRES_HOST: postgres-container
      POSTGRES_PASSWORD: admin
      POSTGRES_PORT: "5432"
      POSTGRES_USER: postgres
    image: aacister/ecommerce-users-microservice:latest
    networks:
      ecommerce-network: null
      users-postgres-network: null
    ports:
      - mode: ingress
        target: 8080
        published: "5000"
        protocol: tcp
networks:
  ecommerce-network:
    name: dockercompose10665008849449285171_ecommerce-network
