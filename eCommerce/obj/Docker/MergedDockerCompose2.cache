      MYSQL_PORT: "3306"
      MYSQL_USER: root
      RabbitMQ_HostName: rabbitmq
      RabbitMQ_Password: password
      RabbitMQ_Port: "5672"
      RabbitMQ_Products_Exchange: product.exchange
